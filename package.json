{
  "name": "tawkee-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && cp dist/widget.bundle.iife.js public/widget.bundle.iife.js && cp dist/widget.js public/widget.js",
    "preview": "vite preview",
    "lint": "set NODE_OPTIONS=--max-old-space-size=4096 && eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "set NODE_OPTIONS=--max-old-space-size=4096 && eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\" --config ./.prettierrc.cjs"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@mui/x-charts": "^8.3.1",
    "@mui/x-data-grid": "^8.2.0",
    "@mui/x-data-grid-pro": "^8.2.0",
    "@mui/x-date-pickers": "^8.2.0",
    "@mui/x-tree-view": "^8.2.0",
    "@react-spring/web": "^9.7.5",
    "dayjs": "^1.11.13",
    "qrcode.react": "^4.2.0",
    "react": "latest",
    "react-dom": "latest",
    "react-router-dom": "^7.5.3",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/node": "^24.1.0",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "latest",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.28.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.32.1",
    "vite": "latest"
  }
}
